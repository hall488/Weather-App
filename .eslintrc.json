{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["webpack.*.js"],
    "rules": {
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ],
        "operator-linebreak":"off",
        "no-plusplus" : "off",
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "no-param-reassign": ["error", { "props": false }]
    }
}
